extends includes/template

block content
    .page
        //head:Start
        div.head
            header.header
                div.wrap
                    ul.navigation
                        -
                            var menu = {'7 причин купить у нас':'#cause',
                                        'О жилом комплексе':'#about',
                                        'Планировка':'#plan',
                                        'Ипотека и рассрочка':'#ipoteka',
                                        'Отзывы клиентов':'#reviews',
                                        'Галерея':'#gallery',
                                        'Контакты':'#contacts'};
                        each link,txt in menu
                            li
                                a(href=link) #{txt}
                            =' '
                    .mobile-menu
                        span
                .overlay
            div.head__bottom
                div.wrap
                    .top
                        .left
                            h1
                                span Жилой комплекс
                                strong Можайский
                        .right
                            a(href='tel:8(395)789-47-81') 8 (395) 789-47-81
                            div
                                span
                                    | Первая очередь
                                    br
                                    | сдана в срок
